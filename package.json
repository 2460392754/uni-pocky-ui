{
    "name": "@pocky/uni-ui",
    "version": "1.0.0",
    "description": "uniapp的ui框架",
    "main": "./src/index.js",
    "scripts": {
        "format": "prettier --write \"src/**/*\"",
        "lint": "eslint --ext .ts,.vue,.js src",
        "sass": "sass ./src/styles/index.scss ./dist/styles/index.css --style compressed --no-source-map",
        "sass:watch": "sass ./src/styles/index.scss ./dist/styles/index.css --watch --style compressed --no-source-map",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "upx2px": "upx2px --watch --target=./dist/styles/index.css --output=./dist/styles/index.px.css"
    },
    "author": "pocky",
    "license": "ISC",
    "devDependencies": {
        "@pocky/uni-core": "^1.0.0",
        "@pocky/uni-upx2px": "^1.0.0",
        "@types/uni-app": "^1.4.1",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.8.0",
        "eslint-config-alloy": "^3.5.0",
        "eslint-plugin-vue": "^6.1.2",
        "prettier": "^1.19.1",
        "sass-loader": "^8.0.0",
        "vue-eslint-parser": "^7.0.0",
        "vuepress": "^1.2.0"
    },
    "dependencies": {}
}
